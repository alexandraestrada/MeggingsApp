<div>
<h1> Sign Up</h1>

<%= form_for @user do |u| %> 
	
	<%= u.label :username %> </br>
	<%= u.text_field :username %> </br>
	<%= u.label :email %> </br>
	<%= u.text_field :email %></br>
	<%= u.label :password %></br>
	<%= u.text_field :password %></br>
	
	<%= u.button "Sign Up" %></br>
<% end %>
</div>

